<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%
	String id = (String) session.getAttribute("id");
%>

<!DOCTYPE html>
<html lang="ko">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>마음가짐 결제 성공페이지</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">    
    </head>


    <body>
        <div class="d-flex align-items-center justify-content-center vh-100">
            <div class="text-center">
                <h1 class="display-1 fw-bold mb">결제완료</h1>
                <span id="boot-icon" class="bi bi-check-circle" style="font-size: 100px; color: rgb(0, 128, 55);"></span>
                <p class="lead">
                 	<span class="text-success fw-bold">결제가</span> 정상적으로 처리되었습니다.
                  </p>
                <a href="/mypage/<%= id %>" class="btn btn-primary">마이페이지로 가기</a>
            </div>
        </div>
    </body>


</html>