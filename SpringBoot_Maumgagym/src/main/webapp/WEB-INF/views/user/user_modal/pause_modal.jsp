<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    
	<!-- The Modal -->
	<div class="modal fade" id="pauseModal">
	  <div class="modal-dialog modal-dialog-centered">
	    <div class="modal-content">
	
	      <!-- Modal Header -->
	      <div class="modal-header">
	        <h4 class="modal-title">중지 확인</h4>
	        <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
	      </div>
	      
	      <!-- Modal body -->
	      <div id="pauseConfirmMessage" class="modal-body">
	      </div>
	
	      <!-- Modal footer -->
	      <div class="modal-footer">
	      	<button id="pauseOk" type="button" class=" btn btn-primary"  data-bs-dismiss="modal">중지하기</button>
	        <button id="pausecancle" type="button" class="btn btn-danger" data-bs-dismiss="modal">취소하기</button>
	      </div>
	
	    </div>
	  </div>
	</div>


